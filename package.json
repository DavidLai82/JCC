{
  "name": "jcc-mombasa-website",
  "version": "1.0.0",
  "description": "Official website for Jesus Celebration Centre (JCC) Mombasa - A modern, responsive church website built with React and TypeScript",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:prod": "tsc && vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run"
  },
  "keywords": [
    "church",
    "jesus-celebration-centre",
    "mombasa",
    "react",
    "typescript",
    "vite",
    "tailwind"
  ],
  "author": "JCC Mombasa Development Team",
  "license": "MIT",
  "dependencies": {
    "@hookform/resolvers": "^3.3.1",
    "@supabase/supabase-js": "^2.39.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.279.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.4",
    "react-router-dom": "^6.15.0",
    "tailwind-merge": "^1.14.0",
    "yup": "^1.7.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/ui": "^1.2.2",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.28",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vitest": "^1.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DavidLai82/JCC.git"
  },
  "bugs": {
    "url": "https://github.com/DavidLai82/JCC/issues"
  },
  "homepage": "https://github.com/DavidLai82/JCC#readme"
}
